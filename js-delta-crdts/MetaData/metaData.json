{
    "crdts": [
        {"crdt":"counter", "functions":[
            {"name":"inc", "rg":"dec", "param":"val"},  
            {"name":"dec", "rg":"inc","param":"val"}
            ], 
        "stat":"mean", "max":"3", "no_op": "value"},

        {"crdt":"set", "functions":[
            {"name":"add", "rg":"remove", "param":"val"},  
            {"name":"remove", "rg":"add","param":"val"}
            ], 
        "stat":"std", "max":"2", "lt_size":"5", "no_op": "value().size"},

        {"crdt":"graph", "functions":[
            {"name":"add_v", "rg":"remove_v", "param":"val"},  
            {"name":"remove_v", "rg":"add_v","param":"val"},
            {"name":"add_e", "rg":"remove_e", "param1":"val", "param2":"val"},  
            {"name":"remove_e", "rg":"add_e","param1":"val", "param2":"val"}
            ], 
        "stat":"dup_edge", "lt_v_size":"50"}
    ]
}